{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
    "type": "string",
    "enum": [
      "ignore-case",
      "match-declaration-order",
      "shorthand-first"
      
    ]
}
